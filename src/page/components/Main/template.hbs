{{#if isTodosNotEmpty}}
  <section class="main">
    <input class="toggle-all" type="checkbox" {{#if checked}}checked{{/if}}>
    <label for="toggle-all">Mark all as completed</label>
    <cat-todo-list></cat-todo-list>
  </section>

  <footer class="footer">
    <cat-counter></cat-counter>
    <cat-filter-list></cat-filter-list>
    <cat-button>
      <span>Clear completed</span>
    </cat-button>
  </footer>
{{/if}}
